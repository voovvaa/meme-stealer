API_ID=123456
API_HASH=your_api_hash
PHONE_NUMBER=+79991234567
TELEGRAM_PASSWORD=
# опционально, если включена двухфакторная аутентификация; оставьте пустым, если её нет
TARGET_CHANNEL_ID=@my_public_channel
SOURCE_CHANNEL_IDS=@meme_channel_one,@meme_channel_two
AD_KEYWORDS=реклама,партнёр,промокод
LOG_LEVEL=info
SESSION_STORAGE_PATH=./sessions/client.session
MEME_DB_PATH=./sessions/memes.sqlite

# Очередь публикаций (опционально)
ENABLE_QUEUE=false
# если true, посты будут публиковаться с задержкой; если false, посты публикуются немедленно
PUBLISH_INTERVAL_MIN=60
# минимальный интервал между публикациями в секундах (по умолчанию: 60 сек = 1 минута)
PUBLISH_INTERVAL_MAX=300
# максимальный интервал между публикациями в секундах (по умолчанию: 300 сек = 5 минут)

# Basic Auth для веб-интерфейса (опционально)
BASIC_AUTH_USER=
# имя пользователя для доступа к веб-интерфейсу; оставьте пустым, чтобы отключить Basic Auth
BASIC_AUTH_PASSWORD=
# пароль для доступа к веб-интерфейсу
